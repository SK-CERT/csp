BEGIN:VCALENDAR
PRODID:-//Mozilla.org/NONSGML Mozilla Calendar V1.1//EN
VERSION:2.0
BEGIN:VEVENT
SEQUENCE:[(${seq})]
STATUS:[(${status})]
UID:[(${uid})]
SUMMARY:[(${summary})]
ORGANIZER;CN=[(${organizer})]:MAILTO:[(${organizerEmail})]
[(${attendee})]CATEGORIES:Meeting
DTSTART:[(${start})]
DURATION:[(${duration})]
TRANSP:OPAQUE
LOCATION:[(${location})]
DESCRIPTION:[(${description})]
BEGIN:VALARM
ACTION:DISPLAY
TRIGGER;VALUE=DURATION:-PT15M
END:VALARM
END:VEVENT
END:VCALENDAR